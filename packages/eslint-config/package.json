{
   "name": "@webdivision/eslint-config",
   "version": "0.0.14-alpha.1",
   "description": "ESLint and Prettier Config from WebDivision",
   "main": "index.js",
   "publishConfig": {
      "access": "public"
   },
   "repository": "git@github.com:webdivisionhq/eslint-config-webdivision.git",
   "license": "MIT",
   "dependencies": {
      "babel-plugin-module-resolver": "^4.0.0",
      "eslint": ">=6.8.0",
      "eslint-config-prettier": "^6.9.0",
      "eslint-plugin-prettier": "^3.1.2",
      "prettier": "^1.19.1"
   },
   "peerDependencies": {
      "eslint-config-kentcdodds": "^14.7.0"
   },
   "scripts": {
      "format": "prettier --write '**/*.js'",
      "lint": "eslint 'src/**/*.{js,jsx}'",
      "lint:fix": "eslint --fix 'src/**/*.{js,jsx}'"
   }
}
