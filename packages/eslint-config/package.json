{
   "name": "@webdivision/eslint-config",
   "version": "0.0.12",
   "description": "ESLint and Prettier Config from WebDivision",
   "main": "index.js",
   "publishConfig": {
      "access": "public"
   },
   "repository": "git@github.com:webdivisionhq/eslint-config-webdivision.git",
   "license": "MIT",
   "dependencies": {
      "babel-plugin-module-resolver": "^3.2.0",
      "eslint": ">=5",
      "eslint-config-prettier": "^4.2.0",
      "eslint-plugin-prettier": "^3.0.1",
      "prettier": "^1.17.0"
   },
   "peerDependencies": {
      "eslint-config-kentcdodds": "^14.3.2"
   },
   "scripts": {
      "format": "prettier --write '**/*.js'",
      "lint": "eslint 'src/**/*.{js,jsx}'",
      "lint:fix": "eslint --fix 'src/**/*.{js,jsx}'"
   }
}
